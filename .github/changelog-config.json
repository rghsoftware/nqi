{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["bug", "bugfix", "fix"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["chore", "maintenance", "ci", "build"]
    },
    {
      "title": "## ♿ Accessibility",
      "labels": ["accessibility", "a11y"]
    }
  ],
  "ignore_labels": ["ignore", "wontfix", "duplicate"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{COMPARE_URL}}",
  "pr_template": "- ${{TITLE}} by @${{AUTHOR}} in #${{NUMBER}}",
  "empty_template": "No changes",
  "max_pull_requests": 100,
  "max_back_track_time_days": 90
}
